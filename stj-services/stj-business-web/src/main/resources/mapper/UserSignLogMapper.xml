<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.stj.business.mapper.UserSignLogMapper">
    <parameterMap id="userSignLogParame" type="com.stj.business.entity.UserSignLog" />
    <resultMap id="userSignLogResult" type="com.stj.business.entity.UserSignLog" />

    <sql id="base_all">
        id, user_id, token, sign_date, expiration_time
    </sql>

    <select id="selectSignLog" parameterType="java.lang.String" resultMap="userSignLogResult">
        select
        <include refid="base_all" />
        from t_user_sign_log
        where token = #{token}
        order by sign_date desc
        limit 0, 1
    </select>
</mapper>
